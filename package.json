{
  "name": "static",
  "version": "1.0.0",
  "description": "Angels Inc static website",
  "main": "src/index.js",
  "engines": {
    "node": "6.2.2"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --config webpack.prod.js --progress && cp _redirects ./dist",
    "cover": "nyc npm run test",
    "lint": "eslint --ext .js --ext .jsx --ignore-pattern *.spec.js src",
    "lint:fix": "npm run lint -- --fix",
    "serve": "NODE_ENV=development webpack-dev-server --config webpack.dev.js --progress",
    "test": "ava src/js/**/*.spec.js",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:angels-Inc/angels-static.git"
  },
  "author": "111Studio K.K",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/angels-Inc/angels-static/issues"
  },
  "homepage": "https://github.com/angels-Inc/angels-static#readme",
  "dependencies": {
    "react": "15.1.0",
    "react-dom": "15.1.0",
    "react-plan": "0.4.2"
  },
  "devDependencies": {
    "ava": "0.15.2",
    "babel": "6.5.2",
    "babel-core": "6.10.4",
    "babel-eslint": "6.1.0",
    "babel-loader": "6.2.4",
    "babel-plugin-react": "1.0.0",
    "babel-plugin-typecheck": "3.9.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "chai": "3.5.0",
    "css-loader": "0.23.1",
    "enzyme": "2.3.0",
    "eslint": "2.10.2",
    "eslint-config-111studio": "1.0.0",
    "eslint-plugin-flowtype": "2.2.7",
    "eslint-plugin-react": "5.2.2",
    "extract-text-webpack-plugin": "1.0.1",
    "happypack": "2.1.1",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.21.0",
    "image-webpack-loader": "1.8.0",
    "jsdom": "9.3.0",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "nyc": "6.6.1",
    "prunk": "1.2.1",
    "react-addons-test-utils": "15.1.0",
    "react-hot-loader": "3.0.0-beta.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
    "webpack-cleanup-plugin": "0.2.0",
    "webpack-dev-server": "1.14.1"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "./ava.conf.js"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      "react-hot-loader/babel",
      [
        "typecheck",
        {
          "disable": {
            "production": true
          }
        }
      ]
    ]
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  },
  "lambda": {
    "version": "0.7.0",
    "lastUpdate": "2016-07-25T08:19:06.328Z"
  }
}
